{
  "compilerOptions": {
    "strict": true,
    "target": "es2022",
    "module": "node16",
    "moduleResolution": "node16",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowImportingTsExtensions": true
  },
  "include": [
    "./src/**/*.ts",
    ".next/types/**/*.ts"
, "src/server.js", "matterServer.js", "global.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
